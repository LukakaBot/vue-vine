{
  "name": "@vue-vine/eslint-config",
  "version": "0.0.2",
  "description": "ESLint config for Vue Vine",
  "author": "ShenQingchuan",
  "license": "MIT",
  "keywords": [
    "Vue",
    "Vine",
    "eslint",
    "eslintConfig"
  ],
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "node": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest --run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@vue-vine/eslint-parser": "workspace:*",
    "@vue-vine/eslint-plugin": "workspace:*",
    "eslint": "~9.6.0"
  }
}
